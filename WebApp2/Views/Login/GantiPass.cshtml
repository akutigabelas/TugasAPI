@{
    ViewData["Title"] = "Ganti Password";
}


<h1>Ganti Password</h1>


<form method="post" asp-action="GantiPass">
    <input name="Email" type="text" />
    <input name="Password" placeholder="password lama" type="password" />
    <input name="confirm" placeholder="password baru" type="password" />
    <button type="submit">Submit</button>
</form>

